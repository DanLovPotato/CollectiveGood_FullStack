<%- include("partials/header") -%>


    <div class="d-flex flex-column flex-md-row p-4 gap-4 py-md-5 align-items-center justify-content-center">
        <div class="list-group list-group-checkable d-grid gap-2 border-0 col-9">
            <% cases.forEach(function(case_){ %>
                <label class="list-group-item rounded-3 py-3 my-1">
                    <h3>
                        Case <%= case_._id%>
                    </h3>
                    <span class="d-block small opacity-50">
                        <h5>
                            <%=case_.diagnostic_results.substring(0,100) + " ..." %>
                                <a href="/case/<%=case_._id%>"> Read more</a>
                        </h5>
                    </span>
                </label>

                <%}) %>

        </div>
    </div>


    <%- include("partials/footer") -%>